<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Location:</b>
  <%= @event.location %>
</p>

<p>
  <b>Timestamp:</b>
  <%= @event.timestamp %>
</p>

<p>
  <b>Parties</b>
  <ul>
    <% @event.parties.each do |party| %>
      <%= link_to party.name, party %>
    <% end %>
  </ul>
</p>


<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
